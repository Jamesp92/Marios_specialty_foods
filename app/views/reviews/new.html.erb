<h1> write a review for: <%= @product.name %></h1>


<%= form_for [@product, @review] do |f| %>
 <%= f.label :author %>
  <%= f.text_field :author %>
   <%= f.label :rating %>
  <%= f.number_field :rating %>
  <%= f.label :content_body %>
  <%= f.text_field :content_body %>
  <%= f.submit  %>
<% end %>


